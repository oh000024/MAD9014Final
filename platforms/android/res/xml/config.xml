<?xml version='1.0' encoding='utf-8'?>
<widget id="ca.edumedia.oh000024.di" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Device">
        <param name="android-package" value="org.apache.cordova.device.Device" />
    </feature>
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="SplashScreen">
        <param name="android-package" value="org.apache.cordova.splashscreen.SplashScreen" />
        <param name="onload" value="true" />
    </feature>
    <name>JakeOh-Sports</name>
    <description> A sample Apache Cordova application that responds to the deviceready event. </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io"> Apache Cordova Team </author>
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <splash density="port-ldpi" src="res/screens/screen-ldpi-portrait.png" />
    <splash density="port-mdpi" src="res/screens/screen-mdpi-portrait.png" />
    <splash density="port-hdpi" src="res/screens/screen-hdpi-portrait.png" />
    <splash density="port-xhdpi" src="res/screens/screen-xhdpi-portrait.png" />
    <splash density="land-ldpi" src="res/screens/screen-ldpi-landscape.png" />
    <splash density="land-mdpi" src="res/screens/screen-mdpi-landscape.png" />
    <splash density="land-hdpi" src="res/screens/screen-hdpi-landscape.png" />
    <splash density="land-xhdpi" src="res/screens/screen-xhdpi-landscape.png" />
    <icon density="port-ldpi" src="res/icons/icon-36-ldpi.png" />
    <icon density="port-mdpi" src="res/icons/icon-48-mdpi.png" />
    <icon density="port-hdpi" src="res/icons/icon-72-hdpi.png" />
    <icon density="port-xhdpi" src="res/icons/icon-96-xhdpi.png" />
    <icon density="port-xxhdpi" src="res/icons/icon-144-xxhdpi.png" />
    <icon density="port-xxxhdpi" src="res/icons/icon-192-xxxhdpi.png" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="SplashScreen" value="screen" />
    <preference name="SplashScreenDelay" value="5000" />
</widget>
